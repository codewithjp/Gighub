

@model IEnumerable<Gighub.Models.Employee>


    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Gender</th>
                <th>Contact</th>
                <th>Department</th>
                <th>Head</th>
            </tr>
        </thead>

        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td> @item.Name</td>
                    <td>@item.Gender</td>
                    <td>@item.MobileNo</td>
                    <td>@item.Department.DepName</td>
                    <td>@item.Department.DepHead</td>
                </tr>
            }
        </tbody>
    </table>